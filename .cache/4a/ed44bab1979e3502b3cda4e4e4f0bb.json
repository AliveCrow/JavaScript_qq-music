 {\n      width: 100%;\n      // height: 400px;\n      text-decoration: none;\n      color: $fontColor;\n      text-align: center;\n      font-size: .2rem;\n      display: -webkit-box;\n      -webkit-box-orient: vertical;\n      -webkit-line-clamp: 3;\n      overflow: hidden;\n      position: relative;\n\n      .img_url {\n        width: 100%;\n        border-radius: 5px;\n        border: 1px solid $fontColor;\n        height: 157px;\n      }\n\n      .img_btn {\n        position: absolute;\n        bottom: 0;\n        right: 0;\n        width: 30px;\n        height: 30px;\n        z-index: 99;\n        border-radius: 50%;\n        padding: 7px;\n        margin-bottom: 3rem;\n        margin-right: 10px;\n        background-color: rgba(0, 0, 0, .5);\n      }\n    }\n  }\n\n  .new_song_item {\n    height: 100%;\n    width: 100px;\n    font-size: 14px;\n\n    a {\n      text-decoration: none;\n      color: $fontColor;\n      font-weight: 200;\n\n      span {\n        font-size: .2em;\n        display: inline-block;\n        padding: 0 5px;\n        display: -webkit-box;\n        -webkit-box-orient: vertical;\n        -webkit-line-clamp: 2;\n        overflow: hidden;\n      }\n    }\n\n    img {\n      border-radius: 8px;\n      height: 100px;\n      width: 100px;\n    }\n  }\n}","scss/_var.scss":"$bodyBgc:#f4f4f4;\n$headerBgc:#f9f9f9;\n$borderColor:#d8d8d8;\n$fontColor:#575656;\n$activeColor:rgba(34, 213, 156, 1);","scss/_animation.scss":"@import 'var';\n\n\n@keyframes move {\n  0% {\n    width: 0;\n    transform: translateX(.7rem);\n  }\n\n  100% {\n    width: 1.4rem;\n    transform: translateX(0);\n  }\n}\n\n@keyframes fontColor {\n  0% {\n    color: $fontColor;\n  }\n\n  100% {\n    color: $activeColor;\n  }\n}","scss/_search.scss":"@import 'var';\n\n.search {\n  margin-left: 16px;\n  margin-right: 16px;\n  margin-top: 16px;\n\n  .search_form {\n    position: relative;\n  }\n\n  img {\n    height: 18px;\n    position: absolute;\n    left: 50%;\n    top: 50%;\n    margin-top: -9px;\n    margin-left: -24px;\n    pointer-events: none\n  }\n\n  input {\n    outline: none;\n    width: 100%;\n    border: none;\n    height: 30px;\n    padding: 5px 0 4px 24px;\n    border-radius: 15px;\n  }\n\n  .search_form_input:focus::-webkit-input-placeholder {\n    text-align: start;\n    cursor: pointer;\n  }\n\n  .search_form_input:focus+img {\n    margin-left: 4px;\n    left: 0;\n  }\n\n  .search_form_input:focus+img+.search_form_button {\n    display: block;\n  }\n\n  input::-webkit-input-placeholder {\n    text-align: center;\n    font-weight: 200;\n    color: $fontColor;\n  }\n\n  .search_form_group {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n\n    .search_form_button {\n      min-width: 2em;\n      margin-left: 16px;\n    }\n  }\n}","scss/_header.scss":"@import 'var';\n\n.header {\n  background-color: $headerBgc;\n  height: 96px;\n\n  .header_box {\n    height: 54px;\n\n  }\n\n  .header_top {\n    display: flex;\n    justify-content: space-between;\n    height: 100%;\n    align-items: center;\n\n    .header_left {\n      display: flex;\n      align-items: center;\n      margin-left: 16px;\n\n      .logo {\n        height: 30px;\n        padding: 3px;\n        background-color: #fff;\n        margin: 5px;\n        border-radius: 5px;\n      }\n    }\n\n    .header_right {\n      cursor: pointer;\n      font-size: .7rem;\n      color: $fontColor;\n      margin-right: 14px;\n      border: solid 1px $borderColor;\n      padding: 4px 20px;\n      border-radius: 50px;\n    }\n\n    .context1 {\n      h2 {\n        font-size: 1em;\n        font-weight: 400;\n      }\n\n      p {\n        font-size: 0.12rem;\n        font-weight: 100;\n      }\n    }\n  }\n\n  .header_tab {\n    height: 42px;\n    background-color: $headerBgc;\n    font-weight: 300;\n\n    ul {\n      width: 100%;\n      display: flex;\n\n      li {\n        line-height: 42px;\n        text-align: center;\n        width: 100%;\n      }\n    }\n  }\n\n  .active {\n    color: $activeColor;\n    position: relative;\n    animation: fontColor .2s ease-in-out forwards;\n\n    &::after {\n      content: '';\n      position: absolute;\n      bottom: 0;\n      left: 50%;\n      margin-left: -.7em;\n      width: 1.4rem;\n      border-radius: .3em;\n      height: .21em;\n      background-color: $activeColor;\n      animation: move .25s ease-in-out forwards;\n    }\n  }\n\n}","scss/_reset.scss":"* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\nhtml,\nbody {\n  font-size: 16px;\n}\n\np,\nh1,\nh2,\nsection {\n  &:hover {\n    cursor: default;\n  }\n}\n\nli {\n  list-style: none;\n}","scss/app.scss":"@import 'slider', 'newSong', 'animation', 'search', 'header', 'var', 'reset';\n\nbody {\n  background-color: $bodyBgc;\n\n  &::-webkit-scrollbar {\n    width: 1px;\n  }\n}\n\n.hidden {\n  display: none;\n}"},"lineCount":null}},"error":null,"hash":"04d38c4991498355576236f448557522","cacheData":{"env":{}}}